    "angular-svg-icon": "^15.0.0",
    "animate.css": "^4.1.1",
    "howler": "^2.2.3",
    "ionicons": "^6.0.3",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.21",
    "ngrx-store-freeze": "^0.2.4",
    "ngrx-store-localstorage": "^15.0.0",
    "openvidu-browser": "^2.20.0",
    "rxjs": "~7.5.0",
