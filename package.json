    "@ionic/angular-toolkit": "^6.0.0",
    "@types/geojson": "^7946.0.10",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
